[Unit]
Description=DGTCentaurMods software.
After=network.target

[Service]
ExecStart=/usr/bin/python3.7 /home/pi/DGTCentaur/DGTCentaurMods/game/menu.py
WorkingDirectory=/home/pi/DGTCentaur
Environment="PYTHONPATH=/home/pi/DGTCentaur"
StandardOutput=inherit
StandardError=inherit
Restart=no
User=pi

[Install]
WantedBy=multi-user.target
